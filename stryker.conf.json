{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "buildCommand": "tsc",
  "reporters": [
    "html",
    "clear-text",
    "progress",
    "dashboard"
  ],
  "testRunner": "mocha",
  "coverageAnalysis": "off",
  "mochaOptions": {
    "spec": ["test/**/*.js"]
  },
  "mutate": [
    "lib/**/*.ts"
  ],
  "dashboard": {
    "project": "github.com/semyonf/node-smpp"
  }
}
